@model IEnumerable<MySchoolV1.Models.UserSubject>
@using MySchoolV1.Models;
@{
    ViewBag.Title = "Student Courses";
}

<h2>StudentCourses</h2>


<p>
</p>

<table class="table">
    <tr>
        <th>ID</th>
        <th>
            Course Name
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @item.ID
        </td>
         <td>
            @Subject.returnSubjectName(item.SubjectID)
        </td>
       
        <td>
            @Html.ActionLink("Edit", "EditUserSubject", new { ID=item.ID }) |
            @Html.ActionLink("Delete", "DeleteUserSubject", new { ID=item.ID })|
            @Html.ActionLink("Create", "CreateUserSubject", new { ID = item.ID }) 
        </td>
    </tr>
}

</table>

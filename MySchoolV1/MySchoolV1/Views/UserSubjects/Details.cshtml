@model MySchoolV1.Models.UserSubject

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div>
    <h4>UserSubject</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.UserID)
        </dt>

        <dd>
            @MySchoolV1.Models.User.returnUserName(Model.UserID)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.SubjectID)
        </dt>

        <dd>
            @MySchoolV1.Models.Subject.returnSubjectName(Model.SubjectID)
